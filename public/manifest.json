{
  "name": "Drawshot",
  "manifest_version": 2,
  "version": "1.0.0",
  "description": "Page painter",
  "content_scripts": [
    {
      "js": ["content.js"],
      "matches": ["<all_urls>"]
    }
  ],
  "background": {
    "scripts": ["background.js"]
  },
  "browser_action": {},
  "permissions": ["contextMenus", "activeTab", "desktopCapture", "downloads"],
  "web_accessible_resources": ["*.html"]
}
