{
  "name": "Drawshot",
  "manifest_version": 2,
  "version": "0.0.1",
  "description": "Page painter",
  "content_scripts": [
    {
      "js": ["content.js"],
      "matches": ["<all_urls>"]
    }
  ],
  "background": {
    "scripts": ["background.js"]
  },
  "permissions": [
    "contextMenus",
    "tabs",
    "desktopCapture",
    "activeTab",
    "tabs",
    "storage",
    "unlimitedStorage",
    "browsingData",
    "notifications",
    "http://*/*",
    "https://*/*",
    "file://*/*",
    "<all_urls>",
    "background",
    "downloads"
  ],
  "web_accessible_resources": ["*.html"]
}
